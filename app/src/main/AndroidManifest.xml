<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.android.bucharest_tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".MainAttractions"
            android:label="Main Attractions">
        </activity>
        <activity
            android:name=".TraditionalRestaurants"
            android:label="Traditional Restaurants">
        </activity>
        <activity
            android:name=".FreeTimeActivities"
            android:label="Free Time Activities">
        </activity>
        <activity
            android:name=".Shopping"
            android:label="Shopping">
        </activity>
        <activity android:name=".AllLocations">
        </activity>
    </application>

</manifest>